{
  "name": "spizian/laravel-env-set",
  "description": "A .env editor for Laravel",
  "homepage": "https://spizian.github.io/laravel-env-set/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adi miuprix",
      "email": "adimiuprix@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "spizian\\EnvSet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spizian\\EnvSet\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.8"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "nunomaduro/larastan": "^2.6",
    "pestphp/pest": "^2.5",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "spizian\\EnvSet\\EnvSetServiceProvider"
      ],
      "aliases": {
        "EnvSet": "spizian\\EnvSet\\Facades\\EnvSet"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
